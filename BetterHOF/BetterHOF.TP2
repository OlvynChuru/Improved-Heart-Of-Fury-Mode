COPY_EXISTING_REGEXP ~.*\.cre~ ~override~
	READ_LONG 0x2bc me_item_offset
	READ_LONG 0x2c0 me_num_items
	me_off=me_item_offset
	FOR (i = 0; i < me_num_items; ++i) BEGIN
		READ_ASCII me_off me_item_name
		PATCH_IF (~%me_item_name%~ STRING_EQUAL_CASE ~MISC07~) BEGIN
			READ_SHORT (me_off + 0xa) me_charges_1
			me_charges_1 = me_charges_1 * 10
			PATCH_IF (me_charges_1 > 32767) BEGIN
				me_charges_1=32000
			END
			WRITE_SHORT (me_off + 0xa) me_charges_1
			READ_SHORT (me_off + 0xc) me_charges_2
			me_charges_2 = me_charges_2 * 10
			PATCH_IF (me_charges_2 > 32767) BEGIN
				me_charges_2=32000
			END
			WRITE_SHORT (me_off + 0xc) me_charges_2
		END
		me_off+=0x14
	END
	BUT_ONLY_IF_IT_CHANGES

